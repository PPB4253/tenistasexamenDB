        "sql": "UPDATE trainer SET trainer_id = $trainer_id, experience = $experience, specialty = $specialty WHERE trainer_id = $trainer_id",
        "request_body_params": [
            "trainer_id",
            "experience",
            "specialty"
        ],
        "auth_required": false,
        "allowed_roles": [
            "*"
        ]
    },
    "delete": {
        "description": "Deletes an existing entry in 'trainer' by its primary key",
        "route": "/trainer/$trainer_id",
