{
    "getAll": {
        "description": "Gets all entries from 'trainer'",
        "route": "/trainer",
        "method": "GET",
        "sql": "SELECT * FROM trainer",
        "auth_required": false,
        "allowed_roles": [
            "*"
        ]
    },
    "getById": {
        "description": "Gets an entry from 'trainer' by its primary key",
